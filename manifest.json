{
  "manifest_version": 2,
  "name": "FPS extension",
  "description": "FPS Extension",
  "version": "1.0",
  "browser_action": {},
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["fps.js"],
      "css": ["fps.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
